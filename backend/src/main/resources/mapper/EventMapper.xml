<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sepay.backend.event.mapper.EventMapper">
    <select id="selectByUserNo" parameterType="int" resultType="eventDTO">
        select *
        from event
        where user_no = #{userNo}
    </select>

</mapper>